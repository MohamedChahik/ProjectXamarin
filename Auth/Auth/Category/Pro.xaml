<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Auth.Category.Pro">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="pro"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button BackgroundColor="Orange" TextColor="White" Text="view post" Clicked="propost_Clicked" x:Name="propost"/>
            <Button BackgroundColor="Orange" TextColor="White" Text="Add post" Clicked="addpropost_Clicked" x:Name="addpropost"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>