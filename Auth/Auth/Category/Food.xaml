<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Auth.Category.Food">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="food"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button BackgroundColor="Orange" TextColor="White" Text="view post" Clicked="foodpost_Clicked" x:Name="foodpost"/>
            <Button BackgroundColor="Orange" TextColor="White" Text="Add post" Clicked="addfoodpost_Clicked" x:Name="addfoodpost"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>